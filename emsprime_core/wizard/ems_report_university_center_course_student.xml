<openerp>
	<data>

		<record id="university_center_course_student_form" model="ir.ui.view">
			<field name="name">ems.report.university.center.course.student.form</field>
			<field name="model">ems.report.university.center.course.student</field>
			<field name="arch" type="xml">
				<form string="Students By Course">
					<group col="4">
						<field name="university_center_id" required="1" options="{'no_open':1, 'no_create': 1}"/>
						<field name="course_id" required="1" options="{'no_open':1, 'no_create': 1}"/>
						<field name="academic_year"/>
						<field name="course_year"/>
						<field name="subject_id" attrs="{'readonly':[('course_id','=',False)]}" context="{'course_subject': True, 'course_id': course_id}"/>
					</group>
					<footer>
						<button name="print_report" type="object" string="Print" class="oe_highlight"/>or
						<button name="cancel" special="cancel" string="cancel" class="oe_link"/>
					</footer>
				</form>
			</field>
		</record>

		<record id="university_center_course_student_action" model="ir.actions.act_window">
			<field name="name">Students List - By Course</field>
			<field name="res_model">ems.report.university.center.course.student</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem id="university_center_course_student_menu"
			action="university_center_course_student_action"
			parent="menu_ems_school_reporting"
			sequence="10"/>

	</data>
</openerp>