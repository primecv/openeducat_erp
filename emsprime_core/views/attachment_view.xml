<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_ems_attachment_tree" model="ir.ui.view">
            <field name="name">ems.attachment.tree</field>
            <field name="model">ems.attachment</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="Attachment">
                    <field name="attachment_type_id" />
                </tree>
            </field>
        </record>
        <record id="view_ems_attachment_form" model="ir.ui.view">
            <field name="name">ems.attachment.form</field>
            <field name="model">ems.attachment</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="Attachment">
                    <sheet>
                        <group col="4">
                            <field name="attachment_type_id" />
							<field name="attachment" filename="dates_fname" widget="binary"/>
							<field name="dates_fname" select="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_ems_attachment_search" model="ir.ui.view">
            <field name="name">ems.attachment.search</field>
            <field name="model">ems.attachment</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="Attachment">
                    <field name="attachment_type_id" />
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_ems_attachment_view">
            <field name="name">Attachments</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ems.attachment</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_ems_attachment_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_ems_attachment_view_form">
            <field name="act_window_id" ref="act_open_ems_attachment_view" />
            <field name="sequence" eval="20" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_ems_attachment_form" />
        </record>
        <record model="ir.actions.act_window.view" id="act_open_ems_attachment_view_tree">
            <field name="act_window_id" ref="act_open_ems_attachment_view" />
            <field name="sequence" eval="10" />
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_ems_attachment_tree" />
        </record>
    </data>
</openerp>