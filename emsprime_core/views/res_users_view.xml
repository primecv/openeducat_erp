<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_users_form_emsprime_core" model="ir.ui.view">
            <field name="name">res.users.form.emsprime.core</field>
            <field name="model">res.users</field>
            <field name="priority">99</field>
            <field name="inherit_id" ref="base.view_users_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='login']" position="after">
					<label for="university_center_id" class="oe_edit_only"/>
                    <field name="university_center_id" />
                </xpath>
            </field>
        </record>		
    </data>
</openerp>
